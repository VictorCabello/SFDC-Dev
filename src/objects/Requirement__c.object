    <validationRules>
        <fullName>No_Grandparents</fullName>
        <active>true</active>
        <description>A Parent Requirement cannot have a Parent Requirement.</description>
        <errorConditionFormula>ISPICKVAL(  Type__c , &quot;Parent Requirement&quot; ) &amp;&amp; ( NOT (ISBLANK( ParentRequirement__c) ) )</errorConditionFormula>
        <errorMessage>A Parent Requirement cannot have a Parent Requirement</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Orphans</fullName>
        <active>true</active>
        <description>Requirements that are not &quot;Parent Requirement&quot; must have a Parent Requirement</description>
        <errorConditionFormula>ISBLANK(  ParentRequirement__c  ) &amp;&amp;  NOT(  ISPICKVAL( Type__c , &quot;Parent Requirement&quot;)  )</errorConditionFormula>
        <errorMessage>A Business Rule, Application Requirement, or Change Request must relate to a Parent Requirement.</errorMessage>
    </validationRules>
